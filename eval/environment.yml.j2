---
home: "{{ home_dir }}"
logging:
  level: DEBUG
  to_file: false
  rotate: false
cleanup:
  auto: true
  keep: 5
paths:
  deps: deps
  logs: logs
  results: results
  temp: temp
  models:
    - "{{ home_dir }}/models"
repos:
  tensorflow:
    url: "https://github.com/tensorflow/tflite-micro.git"
    ref: f868a900f1f788fd0a55b5655b47fc352f9a0239
  etiss:
    url: "https://github.com/tum-ei-eda/etiss.git"
    ref: 78716e7f8e9ee0be0b15d41815d75b35fa2ecaa5
  muriscvnn:
    url: "https://github.com/tum-ei-eda/muriscv-nn.git"
    ref: 04bda19e1d2a829d35394c630878d578eac27b0b
  mlif:
    url: "https://github.com/tum-ei-eda/mlonmcu-sw.git"
    ref: 3f8e66b956fe1cf0d85a18d39d684816e92e2751
frameworks:
  default: tflm
  tflm:
    enabled: true
    backends:
      default: tflmi
      tflmi:
        enabled: true
    features:
      muriscvnn: true
frontends:
  tflite:
    enabled: true
    features:
      validate: true
platforms:
  mlif:
    enabled: true
    features:
      debug: true
      validate: true
targets:
  default: etiss_pulpino
  etiss_pulpino:
    enabled: true
postprocesses:
  use: []
vars:
  runs_per_stage: true
